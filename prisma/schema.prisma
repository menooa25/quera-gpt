// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://env(POSTGRES_USER):env(POSTGRES_PASSWORD)@localhost:5432/env(POSTGRES_DB)?schema=public"
}
